[build-system]
requires = ["sip >=5, <7", "PyQt5"]
build-backend = "sipbuild.api"

[tool.sip.metadata]
name = "@module_name_toml@"

[tool.sip.project]
sip-module = "@sip_name@"
sip-include-dirs = @sip_include_dirs@
sip-files-dir = "@module_srcs@"
abi-version = "12"

[tool.sip.bindings.@module_name_toml@]
tags = @module_tags@
